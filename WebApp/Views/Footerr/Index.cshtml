@model IEnumerable<Entities.Concrete.Footer>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/ManagerPanel/_Layout.cshtml";
}

<br />
<p>
    <a href="/Footerr/Create" class="btn btn-success">Create</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>DESCRIPTION</th>
            <th>UPDATE</th>
            <th>DELETE</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Description</td>
                <td><a href="/Footerr/Bring/@item.Id" class="btn btn-primary">Update</a></td>
                <td><a href="/Footerr/Delete/@item.Id" class="btn btn-danger">Delete</a></td>
            </tr>
}
    </tbody>

</table>

